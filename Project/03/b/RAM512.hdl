/**
 * Memory of 512 registers, each 16 bit-wide. Out holds the value
 * stored at the memory location specified by address. If load==1, then 
 * the in value is loaded into the memory location specified by address 
 * (the loaded value will be emitted to out from the next time step onward).
 */

CHIP RAM512 {
    IN in[16], load, address[9];
    OUT out[16];

    PARTS:
    // Put your code here:
    DMux8Way(in=load, sel=address[6..8], a=loada, b=loadb, c=loadc, d=loadd, e=loade, f=loadf, g=loadg, h=loadh);
    RAM64(in=in, load=loada, address=address[0..5], out=res0);
    RAM64(in=in, load=loadb, address=address[0..5], out=res1);
    RAM64(in=in, load=loadc, address=address[0..5], out=res2);
    RAM64(in=in, load=loadd, address=address[0..5], out=res3);
    RAM64(in=in, load=loade, address=address[0..5], out=res4);
    RAM64(in=in, load=loadf, address=address[0..5], out=res5);
    RAM64(in=in, load=loadg, address=address[0..5], out=res6);
    RAM64(in=in, load=loadh, address=address[0..5], out=res7);
    Mux8Way16(a=res0 ,b=res1 ,c=res2 ,d=res3 ,e=res4 ,f=res5 ,g=res6 ,h=res7 ,sel=address[6..8] ,out=out ); 
}